{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
  "name": "arcdps-unofficial-extras",
  "version-string": "1.29.1",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "magic-enum"
  ],
  "features": {
    "cpp11": {
      "description": "activate compatibility for C++11, will disable magic-enum"
    },
    "magic-enum": {
      "description": "add checks and template specializations for magic-enum.",
      "dependencies": [
        "magic-enum"
      ]
    }
  }
}
